<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Organizer\Fields">
    <fields name="filter">
        <field name="search" type="Blank" label="SEARCH" hint="SEARCH"/>
        <field name="status" type="Options" label="STATUS" class="input-13" onchange="this.form.submit();">
            <option value="">CURRENT_COURSES</option>
            <option value="-1">ALL_COURSES</option>
        </field>
        <field name="termID" type="Terms" label="TERM" class="input-13" onchange="this.form.submit();">
            <option value="">ALL_TERMS</option>
            <option value="-1">NO_TERM</option>
        </field>
        <field name="campusID" type="Campuses" label="CAMPUS" onchange="this.form.submit();">
            <option value="">ALL_CAMPUSES</option>
            <option value="-1">NO_CAMPUS</option>
        </field>
    </fields>
    <fields name="list">
        <field name="limit" type="Limitbox" class="input-mini" default="50" onchange="this.form.submit();"/>
    </fields>
</form>
