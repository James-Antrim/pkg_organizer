<?xml version="1.0" encoding="utf-8"?>
<form>
    <fieldset name="details">
        <field name="id" type="hidden"/>
        <field name="name_de" type="text" label="ORGANIZER_NAME_DE" class="required" maxlength="150" required="true"/>
        <field name="name_en" type="text" label="ORGANIZER_NAME_EN" class="required" maxlength="150" required="true"/>
        <field name="maxValuation" type="Integer" label="ORGANIZER_MAX_SURFACE_PERFORMANCE_VALUE" default="0" first="0" last="300"
               step="10"/>
        <field name="valuation" type="text" label="ORGANIZER_CALCULATED_SURFACE_PERFORMANCE_VALUE" class="required validate-float"
               default="0.00" required="true"/>
        <field name="days" type="text" label="ORGANIZER_CLEANING_DAYS_PER_MONTH" class="required validate-float" default="0.00"
               required="true"/>
        <field name="relevant" type="list" label="ORGANIZER_COST_ACCOUNTING_RELEVANT" default="1">
            <option value="1">ORGANIZER_YES</option>
            <option value="0">ORGANIZER_NO</option>
        </field>
    </fieldset>
</form>
