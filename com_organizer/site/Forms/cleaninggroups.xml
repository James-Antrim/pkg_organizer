<?xml version="1.0" encoding="utf-8"?>
<form>
    <fields name="filter">
        <field name="search" type="text" label="ORGANIZER_SEARCH" class="form-control" hint="SEARCH"/>
        <field name="relevant" type="list" label="ORGANIZER_EXPORTED" onchange="this.form.submit();">
            <option value="">ORGANIZER_COST_ACCOUNTING_IGNORED</option>
            <option value="0">ORGANIZER_COST_ACCOUNTING_IRRELEVANT</option>
            <option value="1">ORGANIZER_COST_ACCOUNTING_RELEVANT</option>
        </field>
    </fields>
    <fields name="list">
        <field name="limit" type="Limitbox" class="input-mini" default="50" onchange="this.form.submit();"/>
    </fields>
</form>
