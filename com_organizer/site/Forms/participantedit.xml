<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Organizer\Fields">
    <fieldset name="details">
        <field name="id" type="hidden"/>
        <field name="referrer" type="hidden"/>
        <field name="forename" type="Blank" required="true" class="input-15 required validate-name" label="FORENAME"/>
        <field name="surname" type="Blank" required="true" class="input-15 required validate-name" label="SURNAME"/>
        <field name="telephone" type="Blank" class="input-15 validate-telephone" label="TELEPHONE"/>
        <field name="address" type="Blank" required="true" class="input-15 required validate-address" label="STREET"/>
        <field name="zipCode" type="Blank" required="true" class="input-15 required validate-alphanumeric"
               label="ZIP_CODE"/>
        <field name="city" type="Blank" required="true" class="input-15 required validate-name" label="RESIDENCE"/>
        <field name="programID" type="Programs" label="PROGRAM" class="input-15" required="true">
            <option value="">SELECT_PROGRAM</option>
        </field>
    </fieldset>
</form>
